{
  "name": "node-express-template",
  "version": "0.0.1",
  "private": true,
  "main": "src/server.ts",
  "author": "Amine Abdelli <amine.abdelli@outlook.fr>",
  "license": "MIT",
  "scripts": {
    "dev": "ts-node-dev -r dotenv/config --no-notify --respawn --exit-child -r tsconfig-paths/register --transpile-only src/server",
    "start": "node -r ts-node/register -r dotenv/config -r tsconfig-paths/register --max_old_space_size=500 ./dist/server.js",
    "prisma:sandbox": "ts-node-dev -r dotenv/config -r tsconfig-paths/register --transpile-only src/sandbox.ts",
    "build": "yarn delete:dist && tsc -p tsconfig.json",
    "delete:dist": "rimraf dist",
    "lint": "eslint **/**.ts",
    "test": "jest"
  },
  "dependencies": {
    "@prisma/client": "^4.12.0",
    "@typescript-eslint/parser": "^5.4.0",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-http-context": "^1.2.4",
    "express-rate-limit": "^6.11.1",
    "firebase": "^10.8.0",
    "fs-extra": "^11.1.1",
    "google-auth-library": "^9.6.3",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "node-mailjet": "^6.0.5",
    "pino": "^7.11.0",
    "pino-pretty": "^7.6.1",
    "prisma": "^4.12.0",
    "socket.io": "^4.5.2",
    "socket.io-client": "^4.5.2",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "ts-node": "^10.8.0",
    "typescript": "5.3.3",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.18.0",
    "@babel/preset-typescript": "^7.17.12",
    "@types/bcryptjs": "^2.4.2",
    "@types/cookie-parser": "^1.4.2",
    "@types/jest": "^29.5.12",
    "@types/jsonwebtoken": "^8.5.5",
    "@types/lodash": "^4.14.202",
    "@types/node": "^16.11.7",
    "@types/socket.io": "^3.0.2",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.6",
    "@types/uuid": "^9.0.4",
    "@typescript-eslint/eslint-plugin": "5.3.1",
    "cross-env": "^7.0.3",
    "eslint": "^8.3.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jest": "^25.2.4",
    "jest": "26.6.0",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^3.11.0",
    "tslint": "^6.1.3"
  }
}